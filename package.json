{
  "name": "bzagc-addict",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate && npm run build:site",
    "prerender:run": "node ./scripts/prerender.js",
    "build:site": "vite build && concurrently \"npm:preview\" \"npm:prerender:run\" --kill-others --success first && node ./scripts/generate-sitemap.js && node ./scripts/generate-rss.js",
    "generate": "node ./scripts/generate-content.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/generative-ai": "^0.16.0",
    "@unhead/vue": "^1.9.16",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "markdown-it": "^14.1.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "puppeteer": "^24.19.0",
    "vite": "^5.3.4",
    "vite-plugin-javascript-obfuscator": "^3.1.0",
    "vite-plugin-ssr-ssg": "^1.4.1"
  }
}
